<?xml version="1.0" encoding="utf-8" ?>
<?xml-stylesheet type="text/xsl" href="/XSLTransform/SHAP.xsl" ?>
<root>
  <input>
    <inputCSV>
        <file>
            <name>{filename}</name>
            <url>{filename}</url> 
        </file>
    </inputCSV>
    <feature_start>{feature_start}</feature_start>
    <feature_end>{feature_end}</feature_end>
    <target>{target}</target>
    <single>{single}</single>
    <interactionA>{interactionA}</interactionA>
    <interactionB>{interactionB}</interactionB>
  </input>
  <output>
    <result>
      <file>
        <name>result.tar.gz</name>
        <url>result.tar.gz</url>
      </file>
    </result>
  </output>
</root>